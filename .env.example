# ========================================
# OpenAnalyst Accountability Coach
# ========================================

# Core Settings
OPENANALYST_DIR=~/.openanalyst
NODE_ENV=development
PORT=3000

# Gemini AI (Claude Code uses this for image/video generation)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-pro
GEMINI_IMAGE_MODEL=imagen-3.0-generate-001
GEMINI_VIDEO_MODEL=veo-001

# Optional
DEBUG=false
LOG_LEVEL=info
